{
  "name": "my-backend",
  "version": "0.0.1",
  "type": "module",
  "description": "my-backend initialised with create-express-typescript-application.",
  "main": "src/index.ts",
  "scripts": {
    "prestart": "yarn build",
    "start": "node ./build/src/index.js",
    "start:dev": "nodemon -w src -w prisma -e ts,mjs,prisma --exec \"tsx ./src/index.ts\"",
    "predev": "yarn tsoa:generate && yarn prisma:sync",
    "dev": "env-cmd -f .env.dev concurrently \"yarn tsoa:generate_\" \"yarn prisma:sync_\" \"yarn start:dev\"",
    "prebuild": "rimraf ./build && yarn tsoa:generate",
    "build": "tsc",
    "build:docker": "rimraf ./build && yarn tsoa:generate && tsc",
    "tsoa:generate_": "nodemon -w src -w prisma -e ts _generate-tsoa.cjs",
    "tsoa:generate": "node _generate-tsoa.cjs",
    "prisma:sync_": "env-cmd -f .env.dev nodemon -w prisma -e prisma,ts _sync-prisma.cjs",
    "prisma:sync": "env-cmd -f .env.dev node _sync-prisma.cjs",
    "prisma:studio": "env-cmd -f .env.dev npx prisma studio",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "express",
    "typescript"
  ],
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "5",
    "@types/express-session": "^1.18.2",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "20",
    "@types/swagger-ui-express": "^4.1.8",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "child_process": "^1.0.2",
    "concurrently": "^6.0.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.32.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "5"
  },
  "dependencies": {
    "@adminjs/express": "6",
    "@adminjs/prisma": "5",
    "@prisma/client": "5",
    "@tiptap/pm": "^3.19.0",
    "@tsoa/runtime": "^7.0.0-alpha.0",
    "adminjs": "7",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.6",
    "env-cmd": "^10.1.0",
    "express": "^4.22.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.19.0",
    "fs": "^0.0.1-security",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^9.0.3",
    "morgan": "^1.10.1",
    "prisma": "5",
    "swagger-ui-express": "^4.1.6",
    "tslib": "^2.8.1",
    "tsoa": "^7.0.0-alpha.0"
  }
}
